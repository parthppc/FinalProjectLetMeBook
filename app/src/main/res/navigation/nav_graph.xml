<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="project.st991591950.dhruvparthtapasvi.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="project.st991591950.dhruvparthtapasvi.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_specialistFragment"
            app:destination="@id/specialistFragment" />
    </fragment>
    <fragment
        android:id="@+id/specialistFragment"
        android:name="project.st991591950.dhruvparthtapasvi.specialist.SpecialistFragment"
        android:label="fragment_specialist"
        tools:layout="@layout/fragment_specialist" />
</navigation>